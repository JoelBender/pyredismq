"""
Subscriber for RedisMQ
"""
from typing import Any

from .debugging import Logging


class Subscriber(Logging):
    """
    Easy wrapper around subscribing
    """

    def __init__(
        self, client: Any, channel, workerCallback, die_after_message: bool = False
    ) -> None:
        """
        default constructor
        """
        self._client = client
        self._channel = channel
        self._workerCallback = workerCallback
        self._die_after_message = die_after_message

    def exit(self) -> None:
        """
        quits subscribing
        """
        return None
